datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id           Int    @id
  name         String @db.VarChar(255)
  username     String @unique
  password     String @db.VarChar(255)
  email        String @db.VarChar(255)
  profileImage String @db.VarChar(255)
  rank Int @unique
  Canvas Canvas[]
}

model Canvas {
  canvasId Int  @id
  users  User[]
}
